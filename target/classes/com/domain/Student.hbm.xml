<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.domain">
	<class name="Student" table="student">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" length="128" not-null="true" />
		</property>
		<property name="pwd" type="java.lang.String">
			<column name="pwd" length="64" not-null="true"></column>
		</property>
		<property name="sex" type="java.lang.String">
			<column name="sex" length="16" not-null="true"></column>
		</property>
		<property name="tel" type="java.lang.String">
			<column name="tel" length="16" not-null="true"></column>
		</property>
		
		<many-to-one name="homePro" column="homePro" />
		<many-to-one name="homeCity" column="homeCity" />
		
		<set name="stuUniversities">
			<key column="student_id" />
			<one-to-many class="com.domain.StuUniversity" />
		</set>
	</class>
</hibernate-mapping>